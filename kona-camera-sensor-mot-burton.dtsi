#include <dt-bindings/clock/qcom,camcc-kona.h>

&tlmm {
	cam_sensor_mclk0_active: cam_sensor_mclk0_active {
		/* MCLK0 */
		mux {
			pins = "gpio94";
			function = "cam_mclk";
		};

		config {
			pins = "gpio94";
			bias-disable; /* No PULL */
			drive-strength = <6>; /* 6 MA */
		};
	};

	cam_sensor_mclk0_suspend: cam_sensor_mclk0_suspend {
		/* MCLK0 */
		mux {
			pins = "gpio94";
			function = "cam_mclk";
		};

		config {
			pins = "gpio94";
			bias-pull-down; /* PULL DOWN */
			drive-strength = <2>; /* 2 MA */
		};
	};

	cam_sensor_mclk1_active: cam_sensor_mclk1_active {
		/* MCLK1 */
		mux {
			pins = "gpio95";
			function = "cam_mclk";
		};

		config {
			pins = "gpio95";
			bias-disable; /* No PULL */
			drive-strength = <6>; /* 6 MA */
		};
	};

    cam_sensor_mclk1_suspend: cam_sensor_mclk1_suspend {
		/* MCLK1 */
		mux {
			pins = "gpio95";
			function = "cam_mclk";
		};

		config {
			pins = "gpio95";
			bias-pull-down; /* PULL DOWN */
			drive-strength = <2>; /* 2 MA */
		};
	};

	cam_sensor_mclk2_active: cam_sensor_mclk2_active {
		/* MCLK2 */
		mux {
			pins = "gpio96";
			function = "cam_mclk";
		};

		config {
			pins = "gpio96";
			bias-disable; /* No PULL */
			drive-strength = <6>; /* 6 MA */
		};
	};

	cam_sensor_mclk2_suspend: cam_sensor_mclk2_suspend {
		/* MCLK2 */
		mux {
			pins = "gpio96";
			function = "cam_mclk";
		};

		config {
			pins = "gpio96";
			bias-pull-down; /* PULL DOWN */
			drive-strength = <2>; /* 2 MA */
		};
	};

	cam_sensor_mclk3_active: cam_sensor_mclk3_active {
		/* MCLK3 */
		mux {
			pins = "gpio97";
			function = "cam_mclk";
		};

		config {
			pins = "gpio97";
			bias-disable; /* No PULL */
			drive-strength = <6>; /* 6 MA */
		};
	};

	cam_sensor_mclk3_suspend: cam_sensor_mclk3_suspend {
		/* MCLK3 */
		mux {
			pins = "gpio97";
			function = "cam_mclk";
		};

		config {
			pins = "gpio97";
			bias-pull-down; /* PULL DOWN */
			drive-strength = <2>; /* 2 MA */
		};
	};

	cam_flash_en_default: cam_flash_en_default {
		/* RESET */
		mux {
			pins = "gpio98";
			function = "gpio";
		};

		config {
			pins = "gpio98";
			drive-strength = <2>; /* 2 MA */
			output-low;
		};
	};
};

&soc {
	qcom,cam-res-mgr {
		compatible = "qcom,cam-res-mgr";
		status = "ok";
	};

	camera_front_avdd: gpio-regulator@0{
		compatible = "regulator-fixed";
		regulator-name = "camera_front_avdd";
		regulator-min-microvolt = <2800000>;
		regulator-max-microvolt = <2800000>;
		regulator-enable-ramp-delay = <135>;
		enable-active-high;
		gpio = <&pm8150l_gpios 3 0>;
	};

	camera_front_dvdd: gpio-regulator@1{
		compatible = "regulator-fixed";
		regulator-name = "camera_front_dvdd";
		regulator-min-microvolt = <1050000>;
		regulator-max-microvolt = <1050000>;
		regulator-enable-ramp-delay = <135>;
		enable-active-high;
		gpio = <&pm8150l_gpios 4 0>;
	};

	camera_flash_en_gpio: gpio-regulator@4 {
		compatible = "regulator-fixed";
		reg = <0x04 0x00>;
		regulator-name = "camera_flash_en_gpio";
		regulator-min-microvolt = <1808000>;
		regulator-max-microvolt = <1808000>;
		regulator-enable-ramp-delay = <233>;
		enable-active-high;
		gpio = <&tlmm 98 0>;
		pinctrl-names = "default";
		pinctrl-0 = <&cam_flash_en_default>;
	};

	dummy_cam_vreg: dummy_cam_vreg {
		compatible = "regulator-fixed";
		status = "ok";
		regulator-name = "dummy_cam_vreg";
		regulator-min-microvolt = <2800000>;
		regulator-max-microvolt = <2800000>;
	};
};

&cam_cci1 {
		pinctrl-0 = <&cci2_active>;
		pinctrl-1 = <&cci2_suspend>;
		gpios = <&tlmm 105 0>,
			<&tlmm 106 0>;
		gpio-req-tbl-num = <0 1>;
		gpio-req-tbl-flags = <1 1>;
		gpio-req-tbl-label = "CCI_I2C_DATA2",
					"CCI_I2C_CLK2";

		actuator_rear: qcom,actuator@0 {
			cell-index = <0>;
			reg = <0x0>;
			compatible = "qcom,actuator";
			cci-master = <0>;
			cam_vaf-supply = <&cam_main_afois_vdd_3p5>;
			regulator-names = "cam_vaf";
			rgltr-cntrl-support;
			rgltr-min-voltage = <2800000>;
			rgltr-max-voltage = <2800000>;
			rgltr-load-current = <0>;
		};

		eeprom_rear: qcom,eeprom@0 {
			cell-index = <0>;
			reg = <0x0>;
			compatible = "qcom,eeprom";
			cci-master = <0>;
			cam_vio-supply = <&pm8150a_l5>;
			regulator-names = "cam_vio";
			rgltr-cntrl-support;
			rgltr-min-voltage = <1800000>;
			rgltr-max-voltage = <1800000>;
			rgltr-load-current = <14000>;
		};

		ois_main: qcom,ois@0 {
			cell-index = <0>;
			reg = <0x0>;
			compatible = "qcom,ois";
			cci-master = <0>;
			cam_vio-supply = <&pm8150a_l5>;
			cam_vaf-supply = <&cam_main_afois_vdd_3p5>;
			regulator-names = "cam_vio", "cam_vaf";
			rgltr-cntrl-support;
			rgltr-min-voltage = <1800000 2800000>;
			rgltr-max-voltage = <1800000 2800000>;
			rgltr-load-current = <14000 15000>;
			status = "ok";
		};

		main_sensor: qcom,cam-sensor@0 {
			cell-index = <0>;
			compatible = "qcom,cam-sensor";
			reg = <0x00>;
			csiphy-sd-index = <0>;
			sensor-position-roll = <90>;
			sensor-position-pitch = <0>;
			sensor-position-yaw = <180>;
			led-flash-src = <&led_flash_rear>;
			actuator-src = <&actuator_rear>;
			eeprom-src = <&eeprom_rear>;
			ois-src = <&ois_main>;
			cam_vio-supply = <&pm8150a_l5>;
			cam_vana-supply = <&cam_main_avdd_2p8>;
			cam_vdig-supply = <&cam_main_dvdd_1p05>;
			cam_clk-supply = <&titan_top_gdsc>;
			regulator-names = "cam_vio", "cam_clk", "cam_vana", "cam_vdig";
			rgltr-cntrl-support;
			rgltr-min-voltage = <1800000 0 2800000 1050000>;
			rgltr-max-voltage = <1800000 0 2800000 1050000>;
			rgltr-load-current = <105000 0 120000  105000>;
			gpio-no-mux = <0>;
			pinctrl-names = "cam_default", "cam_suspend";
			pinctrl-0 = <&cam_sensor_mclk0_active
					&cam_sensor_active_rear>;
			pinctrl-1 = <&cam_sensor_mclk0_suspend
					&cam_sensor_suspend_rear>;
			gpios = <&tlmm 94 0>,
				<&tlmm 93 0>;
			gpio-reset = <1>;
			gpio-req-tbl-num = <0 1>;
			gpio-req-tbl-flags = <1 0>;
			gpio-req-tbl-label = "CAMIF_MCLK0",
					"CAM_RESET0";
			sensor-mode = <0>;
			cci-master = <0>;
			status = "ok";
			clocks = <&clock_camcc CAM_CC_MCLK0_CLK>;
			clock-names = "cam_clk";
			clock-cntl-level = "turbo";
			clock-rates = <24000000>;
		};
};

&cam_cci0 {
	actuator_wide: qcom,actuator@1 {
		cell-index = <1>;
		reg = <0x1>;
		compatible = "qcom,actuator";
		cci-master = <0>;
		cam_vaf-supply = <&cam_wfov_tele_afvdd_2p8>;
		regulator-names = "cam_vaf";
		rgltr-cntrl-support;
		rgltr-min-voltage = <2800000>;
		rgltr-max-voltage = <2800000>;
		rgltr-load-current = <0>;
	};

	actuator_tele: qcom,actuator@2 {
		cell-index = <2>;
		reg = <0x2>;
		compatible = "qcom,actuator";
		cci-master = <1>;
		cam_vaf-supply = <&cam_wfov_tele_afvdd_2p8>;
		regulator-names = "cam_vaf";
